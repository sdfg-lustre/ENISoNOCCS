<?xml version="1.0"?>
<sdf3 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" type="sdf" xsi:noNamespaceSchemaLocation="http://www.es.ele.tue.nl/sdf3/xsd/sdf3-sdf.xsd">
  <applicationGraph>
    <sdf name="buffercycle" type="BufferCycle">
      <actor name="a" type="A">
        <port name="p1" type="in" rate="2"/>
        <port name="p2" type="out" rate="1"/>
        <port name="_p3" type="out" rate="1"/>
        <port name="_p4" type="in" rate="1"/>
      </actor>
      <actor name="b" type="B">
        <port name="p1" type="in" rate="1"/>
        <port name="p2" type="out" rate="2"/>
        <port name="_p3" type="out" rate="1"/>
        <port name="_p4" type="in" rate="1"/>
      </actor>
      <actor name="c" type="C">
        <port name="p1" type="in" rate="1"/>
        <port name="p2" type="out" rate="2"/>
        <port name="_p3" type="out" rate="1"/>
        <port name="_p4" type="in" rate="1"/>
      </actor>
      <actor name="d" type="D">
        <port name="p1" type="in" rate="2"/>
        <port name="p2" type="out" rate="1"/>
        <port name="_p3" type="out" rate="1"/>
        <port name="_p4" type="in" rate="1"/>
      </actor>
      <channel name="alpha" srcActor="a" srcPort="p2" dstActor="b" dstPort="p1"/>
      <channel name="beta" srcActor="b" srcPort="p2" dstActor="c" dstPort="p1"/>
      <channel name="gamma" srcActor="c" srcPort="p2" dstActor="d" dstPort="p1"/>
      <channel name="delta" srcActor="d" srcPort="p2" dstActor="a" dstPort="p1" initialTokens="4"/>
      <channel name="_ch5" srcActor="a" srcPort="_p3" dstActor="a" dstPort="_p4" initialTokens="1"/>
      <channel name="_ch6" srcActor="b" srcPort="_p3" dstActor="b" dstPort="_p4" initialTokens="1"/>
      <channel name="_ch7" srcActor="c" srcPort="_p3" dstActor="c" dstPort="_p4" initialTokens="1"/>
      <channel name="_ch8" srcActor="d" srcPort="_p3" dstActor="d" dstPort="_p4" initialTokens="1"/>
    </sdf>
    <sdfProperties>
      <actorProperties actor="a">
        <processor type="p1" default="true">
          <executionTime time="1"/>
        </processor>
      </actorProperties>
      <actorProperties actor="b">
        <processor type="p1" default="true">
          <executionTime time="2"/>
        </processor>
      </actorProperties>
      <actorProperties actor="c">
        <processor type="p1" default="true">
          <executionTime time="1"/>
        </processor>
      </actorProperties>
      <actorProperties actor="d">
        <processor type="p1" default="true">
          <executionTime time="1"/>
        </processor>
      </actorProperties>
      <channelProperties channel="alpha"/>
      <channelProperties channel="beta"/>
      <channelProperties channel="gamma"/>
      <channelProperties channel="delta"/>
      <channelProperties channel="_ch5"/>
      <channelProperties channel="_ch6"/>
      <channelProperties channel="_ch7"/>
      <channelProperties channel="_ch8"/>
      <graphProperties/>
    </sdfProperties>
  </applicationGraph>
</sdf3>
