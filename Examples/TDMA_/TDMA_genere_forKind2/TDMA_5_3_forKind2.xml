<sdf name="example" nberBit="8" maxValue="15">
  
   <actor name="A" duration="5" maxAutoConcurrences="5" maxStages="5">
        <port name="e1" type="in" rate="1"/>
        <port name="e3in" type="in"  rate="1" />
        <port name="e3out" type="out"  rate="1" />
        <port name="e4" type="out"  rate="1" />
    </actor>

    <actor name="A1" duration="3" maxAutoConcurrences="6" maxStages="3">
        <port name="e4" type="in"  rate="1" />
        <port name="e2" type="out" rate="1" />
 
     </actor>

 
    <channel name = "e3" srcActor="A" srcPort="e3out" dstActor="A" dstPort="e3in" initialTokens="5"/>
    <channel name = "e4" srcActor="A" srcPort="e4" dstActor="A1" dstPort="e4" initialTokens="0"/>

    <driver name="e1" actorDest="A" portDest="e1"/>
    <monitor name="e2" srcActor="A1" portSrc="e2"/>
 
 </sdf>
