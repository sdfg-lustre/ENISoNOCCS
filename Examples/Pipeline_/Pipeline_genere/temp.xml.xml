    </actor>
  
 
    <channel name = "Vbis" srcActor="WaitBisBis" srcPort="Vout" dsTaBisctor="WaitBisBis" dstPort="Vin" initialTokens="1"/>    
 
    <channel name = "Wbis" srcActor="TaBis" srcPort="W" dstActor="WaitBis" dstPort="W" initialTokens="3"/>  
    <channel name = "Zbis" srcActor="WaitBis" srcPort="Z" dstActor="TaBis" dstPort="Z" initialTokens="0"/>  
    <driver name="A" actorDest="WaitBis" portDest="A"/>
     
   
 
 </sdf>
