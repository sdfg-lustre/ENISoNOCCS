       <port name="e3out" type="out"  rate="1" />
         <port name="e4" type="out"  rate="1" />
    </actor>

  <actor name="a2" duration="2" maxAutoConcurrences="4">
        <port name="e4" type="in" rate="1"/>
       <port name="e5in" type="in"  rate="1" />
