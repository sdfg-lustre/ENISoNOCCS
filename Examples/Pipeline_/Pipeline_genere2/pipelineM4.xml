    </actor>

<actor name="end" duration="1" maxAutoConcurrences="1">
        <port name="e2" type="in" rate="1"/>
    </actor>
  
    <channel name = "e2" srcActor="a2" srcPort="e2" dstActor="end" dstPort="e2" initialTokens="0"/>  
    <channel name = "e3" srcActor="a1" srcPort="e3out" dstActor="a1" dstPort="e3in" initialTokens="3"/>  
    <channel name = "e4" srcActor="a1" srcPort="e4" dstActor="a2" dstPort="e4" initialTokens="0"/>  
    <channel name = "e5" srcActor="a2" srcPort="e5out" dstActor="a2" dstPort="e5in" initialTokens="1"/>    
    <driver name="e1" actorDest="a1" portDest="e1"/>
     
   
 
 </sdf>
